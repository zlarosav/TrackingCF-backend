{
  "name": "trackingcf-backend",
  "version": "1.0.0",
  "description": "Backend API for TrackingCF - Codeforces Tracking System",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "db:init": "node src/scripts/initDb.js",
    "db:delete": "node src/scripts/deleteDb.js",
    "user:create": "node src/scripts/createUser.js",
    "user:delete": "node src/scripts/deleteUser.js",
    "user:rename": "node src/scripts/renameUser.js",
    "user:toggle": "node src/scripts/toggleUser.js",
    "admin:create": "node src/scripts/createAdmin.js",
    "admin:delete": "node src/scripts/deleteAdmin.js",
    "tracker:run": "node src/scripts/runTracker.js",
    "tracker:force": "node src/scripts/runTracker.js --force",
    "contests:update": "node src/scripts/updateContests.js"
  },
  "keywords": [
    "codeforces",
    "tracking",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.40.0",
    "axios": "^1.7.7",
    "bcryptjs": "^3.0.3",
    "cheerio": "^1.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.3",
    "luxon": "^3.5.0",
    "mysql2": "^3.11.0",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  }
}
